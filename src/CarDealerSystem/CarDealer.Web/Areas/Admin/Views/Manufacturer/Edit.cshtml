@model CarDealer.Services.Models.Manufacturer.ManufacturerUpdateServiceModel

<h2 style="text-align: center">Edit "@Model.Name"</h2>

<hr />
<div class="container">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input readonly="readonly" asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-warning" />
            </div>
        </form>
</div>

<div class="text-md-center" style="margin-bottom: 15px;">
    <a class="btn btn-default" asp-action="Index">Back to List</a>
</div>

